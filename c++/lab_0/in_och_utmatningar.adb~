with Ada.Text_IO;		  use Ada.Text_IO;
with Ada.Integer_Text_IO;

procedure Mycket_Enkel_In_Och_Utmatning is
  
  X : Integer;

begin
  Put("Mata in ett heltal: ");
  Ada.Integer_Text_IO.Get(X);
  Ada.Text_IO.Put("Du skrev ");
  Ada.Integer_Text_IO.Put(X, Width => 1);
end Mycket_Enkel_In_Och_Utmatning;
